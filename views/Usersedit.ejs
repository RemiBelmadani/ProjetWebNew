<%- include("partials/header") %> 
 <!DOCTYPE html>
 <html lang="en">
 <head>
     <meta charset="UTF-8">
     <meta http-equiv="X-UA-Compatible" content="IE=edge">
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <link rel="stylesheet" href="">
 
 </head>
 <body>
     <h1>Single user editor form</h1>
 
 <p><a href='/ADMIN/list'>Back to the list</a></p>
<form action='/Users/update/<%= OneUsers.Users_ID %>' method='post' 
     enctype='application/x-www-form-urlencoded'>
 <table class="table table-striped table-bordered table-hover">
     <tr><td>User name:</td>
         <td><input type='text' name='Users_name' value='<%= OneUsers.Users_name %>' /></td></tr>
     <tr><td>Mail:</td>
         <td><input type='Text' name='Users_mail' value='<%= OneUsers.Users_mail %>' /></td></tr>
     <tr><td>Phone:</td>
         <td><input type='text' name='Users_phone' value='<%= OneUsers.Users_phone %>' /></td></tr>
     <tr><td>Address:</td>
         <td><input type='text' name='Users_address' value='<%= OneUsers.Users_address %>' /></td></tr>
     <tr><td>Password:</td>
         <td><input type='text' name='Users_passwords' value='<%= OneUsers.Users_passwords %>' /></td></tr>

     <tr><td colspan='2'> <input type='submit' value='SEND' /> </td></tr>
 </table>
 </form>
 </body>
 </html>
 <%- include("partials/footer") %>