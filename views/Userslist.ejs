<%- include("partials/header") %>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="">

    <title>User list</title>
</head>
<body>
    <h1>List of Users</h1>

<p><a href='/Users/edit/0'>Add a new User</a></p>

<table class="table-design">
    <tr>
        <th>Name</th>
        <th>Mail</th>
        <th>Phone</th>
        <th>Address</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>

<% for (var c of Users) { %>
    <tr>
        <td><%= c.Users_name %></td>
        <td><%= c.Users_mail %></td>
        <td><%= c.Users_phone %></td>
        <td><%= c.Users_address %></td>
        <td><a href='/Users/edit/<%= c.Users_ID %>'>Edit</a></td>
        <td><a href='/Users/del/<%= c.Users_ID %>'>Delete</a></td>
    </tr>
<% } %>
</table>
</body>
</html>

<%- include("partials/footer") %>